@model IEnumerable<City>

@{
    ViewData["Title"] = "Cities ";
}


<h1> Cities </h1>

<div>
    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("Add", "IDCities")'">Add City</button>
</div>

<div id="errorEFMessages"></div>

<div class="container" id="fet">
    <div class="row">
        <div class="col-sm-2"> Name </div>
        <div class="col-sm-2"> Country </div>
        <div class="col-sm-2"> Citizens </div>
        <div class="col-sm-2"> Details </div>
        <div class="col-sm-2"> Delete </div>
    </div>
</div>

@if (Model.Any() == true)
{
    @foreach (var item in Model)
    {
        <partial name="_cityIDPartial" model="item" />
    }
}