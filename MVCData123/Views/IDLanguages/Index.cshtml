@model IEnumerable<Language>

@{
    ViewData["Title"] = "Languages ";
}


<h1> Languages </h1>

<div>
    <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("Add", "IDLanguages")'">Add Language</button>
</div>

<div id="errorEFMessages"></div>

<div class="container" id="fet">
    <div class="row">
        <div class="col-sm-2"> Name </div>
        <div class="col-sm-2"> Speakers </div>
        <div class="col-sm-2"> Details </div>
        <div class="col-sm-1"> Delete </div>
    </div>
</div>


@if (Model.Any() == true)
{
    @foreach (var item in Model)
    {
        <partial name="_languageIDPartial" model="item" />
    }
}
